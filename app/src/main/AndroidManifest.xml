<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.n00146163.httpurlconnection">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.n00146163.httpurlconnection.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.example.n00146163.httpurlconnection.Service.MyIntentService"
            android:exported="false" />

        <activity
            android:name="com.example.n00146163.httpurlconnection.ASyncTaskActivity"
            android:parentActivityName="com.example.n00146163.httpurlconnection.MainActivity" />
        <activity
            android:name="com.example.n00146163.httpurlconnection.IntentActivity"
            android:parentActivityName="com.example.n00146163.httpurlconnection.MainActivity" />
        <activity
            android:name="com.example.n00146163.httpurlconnection.ASyncTaskLoaderActivity"
            android:parentActivityName="com.example.n00146163.httpurlconnection.MainActivity"></activity>
    </application>

</manifest>